{
  "name": "crunch",
  "version": "1.0.0",
  "description": "A library for anaylizing data sets",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha --exit",
    "coverage": "cross-env NODE_ENV=test nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryandabler/crunch.git"
  },
  "author": "Ryan Dabler <r.p.dabler@gmail.com> (https://ryandabler.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryandabler/crunch/issues"
  },
  "homepage": "https://github.com/ryandabler/crunch",
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.6",
    "mocha": "^5.2.0",
    "nyc": "^12.0.1"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  }
}
